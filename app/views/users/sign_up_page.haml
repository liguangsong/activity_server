%h1
  活动通
%p(class="header-right"style="margin-top:1%")
  你好，
  =session[:user]
  =link_to '退出',admins_quit_path
%div(style="background-color:#e8e8e8;width:700px;height:50px;width:100%" )
  %div(style="height:25px;margin-top:12px;position:absolute;margin-left:3%" )
    =link_to '首页' ,user_path(@id)
    %span(style="color:gray") / 报名列表
%div(class="index-action" style="width:1050px;height:480px;margin-left:10%;margin-top:2%;padding:0px" )
  %div(style="margin-left:1px;margin-top:10px;background-color:#F0F0F0;height:50px"  )
    %span(class="header-left" style="font-size:16px;margin-top:8px;margin-left:10px" )
      报名列表
    %div(class="header-right" style="font-size:16px;margin-top:8px;margin-right:10px")
      =link_to "竞价列表",users_bid_list_page_path(@activity_name)
  %table(class="table table-style"style="margin-top:1px;position:absolute;top:7%" )
    %tbody
      %tr
        %th 编号
        %th 姓名
        %th 手机号
      %br
        %ul
          -i=0
          -@sign_up.each do |t|
            %tr
              -i=i+1;
              %th
                =i
              %th
                =t.name
              %th
                =t.phone

            %br
    %div(style='bottom:-10px;width:100%;height:32px;position:absolute;')
      %div(style="font-size:12px;right:5%;position:absolute;" )
        =will_paginate@sign_up ,{:previous_label =>'<<',:next_label => '>>'}
